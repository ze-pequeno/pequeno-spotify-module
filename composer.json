{
    "name": "ze-pequeno/pequeno-spotify-module",
    "description": "Zend Framework 2 Module for Spotify Web API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "service",
        "spotify",
        "module",
        "zf2"
    ],
    "homepage": "https://github.com/ze-pequeno/pequeno-spotify-module",
    "authors": [
        {
            "name": "Cyril Martin",
            "email": "cyril.martin.cm@gmail.com",
            "homepage": "http://ze-pequeno.github.com/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
	    "zendframework/zend-http": "2.*",
	    "zendframework/zend-mvc": "2.*",
	    "zendframework/zend-serializer": "2.*",
	    "zendframework/zend-servicemanager": "2.*",
	    "zendframework/zend-version": "2.*"
    },
	"require-dev": {
        "phpunit/phpunit": "~3.7",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls": "~0.6"
	},
    "autoload": {
        "psr-0": {
            "PequenoSpotifyModule": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
	"extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}